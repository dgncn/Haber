    <div class="container">
        <!-- start header section -->
        <header id="header">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="header_top">
                        <div class="header_top_left">
                            <ul class="top_nav">
                                <li><a href="home.html">Anasayfa</a></li>
                                <li><a href="#">Hakkında</a></li>
                                <li><a href="contact.html">ILETISIM</a></li>
                                <li><a href="~/Dashboard">Dash</a></li>
                            </ul>
                        </div>
                        <div class="header_top_right">
                            <p>@DateTime.Now.ToShortDateString() - @DateTime.Now.ToShortTimeString()</p>
                        </div>
                    </div>
                </div>
                <div class="col-lg-12 col-md-12 col-sm-12">
                    <div class="header_bottom">
                        <div class="logo_area">
                            <!-- for your img logo format
                            <a href="home.html" class="logo">
                              <img src="~/Gallery/img/logo.jpg" alt="logo">
