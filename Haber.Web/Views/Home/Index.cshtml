@section FooterEtiketleri{
    
    @foreach (var etiket in (IEnumerable<EtiketSonuc>)ViewBag.etiketResult2)
    {
        int a = etiket.EticketSonucSayisi;
        string buyukluk = "";
        if (a==1)
        {
            buyukluk = "cokdusuk";
        }
        else if(a>=2 && a<=4)
        {
            buyukluk = "dusuk";
        }
        else if(a>4 && a<=7)
        {
            buyukluk = "orta";
        }
        else if (a>7 && a<=10)
        {
            buyukluk = "buyuk";
        }
        else
        {
            buyukluk = "cokbuyuk";
        }
        switch (buyukluk)
        {
            case "cokdusuk":
                <li class=""><h5>@etiket.EtiketSonucAdi</h5></li>
                break;
            case "dusuk":
                <li><h4>@etiket.EtiketSonucAdi</h4></li>
                break;
            case "orta":
                <li><h3>@etiket.EtiketSonucAdi</h3></li>
                break;
            case "buyuk":
                <li><h2>@etiket.EtiketSonucAdi</h2></li>
                break;
            case "cokbuyuk":
                <li><h1><b>@etiket.EtiketSonucAdi</b></h1></li>
                break;
            default:
                <li><h5>@etiket.EtiketSonucAdi</h5></li>
                break;
        }
    }

}






